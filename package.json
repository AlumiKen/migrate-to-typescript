  },

  "scripts": {
    "install": "bower install",
    "postinstall": "typings install",
    "prestart": "npm run tsc",
    "start" : "concurrently \"npm run tsc:w\" \"npm run server\" ",
    "server": "node server/server.js",
    "tsc"   : "tsc",
    "tsc:w" : "tsc -w",
    "tscPublic": "tsc –p public",
    "tscServer": "tsc –p server"
  },

